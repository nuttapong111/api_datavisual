{
  "name": "api-looker-dashboard",
  "version": "1.0.0",
  "description": "API service for data analysis and Looker Studio dashboard",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "start:all": "./start.sh",
    "test:api": "curl -s http://localhost:3001/api/upload/status",
    "upload:sample": "curl -X POST -F \"file=@sample_data.csv\" http://localhost:3001/api/upload/excel"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "sqlite3": "^5.1.6",
    "sequelize": "^6.35.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["api", "dashboard", "data-analysis", "excel", "looker"],
  "author": "Your Name",
  "license": "MIT"
}
