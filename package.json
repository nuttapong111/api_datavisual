{
  "name": "api-looker-dashboard",
  "version": "1.0.0",
  "description": "API service for data analysis and Looker Studio dashboard",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:prod": "cd client && npm run build",
    "build:server": "echo 'Server build completed'",
    "start": "node server/index.js",
    "start:prod": "npm run build && node server/index.js",
    "start:all": "./start.sh",
    "test:api": "curl -s http://localhost:3001/api/upload/status",
    "upload:sample": "curl -X POST -F \"file=@sample_data.csv\" http://localhost:3001/api/upload/excel",
    "db:create": "node server/scripts/run-sql.js",
    "db:init": "node server/scripts/init-postgres.js",
    "db:create-railway": "node railway-create-tables.js",
    "force-restart": "node FORCE_RESTART.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "api",
    "dashboard",
    "data-analysis",
    "excel",
    "looker"
  ],
  "author": "Your Name",
  "license": "MIT"
}